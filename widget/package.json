{
  "name": "<<APP_NAME_WITH_SPACE>>",
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . <<APP_NAME>> --overwrite --platform=darwin --arch=x64 --icon=app_icon.icns --electron-version=1.7.9",
    "installer-mac": "electron-installer-dmg <<APP_NAME>>-darwin-x64/<<APP_NAME>>.app <<APP_NAME>>",
    "package-win": "electron-packager . PublisherPlatform --overwrite --platform=win32 --arch=x64 --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PublisherPlatform\" ",
    "package-linux": "electron-packager . PublisherPlatform --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/icon-badge.png --electron-version=1.7.9"
  },
  "dependencies": {
    "electron-modal": "^1.0.0",
    "electron-positioner": "^3.0.0",
    "latest-version": "^3.1.0"
  },
  "devDependencies": {
    "electron": "1.7.9",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^10.1.0"
  }
}
