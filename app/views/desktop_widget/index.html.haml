%table.table
  %thead
    %tr
      %td Client ID 
      %td App Name
      %td Version 
      %td Time
      %td Download
  %tbody
    - @widgets.each do |widget|
      %tr
        %td= widget.client_id
        %td= widget.app_name
        %td= widget.version
        %td= widget.created_at.to_s
        %td= link_to "Download", widget.app.url
